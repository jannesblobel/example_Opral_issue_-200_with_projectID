#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*) basedir=`cygpath -w "$basedir"`;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="/Users/jannesblobel/Documents/GitHub.nosync/monorepo/inlang/source-code/paraglide/paraglide-js/bin/node_modules:/Users/jannesblobel/Documents/GitHub.nosync/monorepo/inlang/source-code/paraglide/paraglide-js/node_modules:/Users/jannesblobel/Documents/GitHub.nosync/monorepo/inlang/source-code/paraglide/node_modules:/Users/jannesblobel/Documents/GitHub.nosync/monorepo/inlang/source-code/node_modules:/Users/jannesblobel/Documents/GitHub.nosync/monorepo/inlang/node_modules:/Users/jannesblobel/Documents/GitHub.nosync/monorepo/node_modules:/Users/jannesblobel/Documents/GitHub.nosync/node_modules:/Users/jannesblobel/Documents/node_modules:/Users/jannesblobel/node_modules:/Users/node_modules:/node_modules:/Users/jannesblobel/Documents/GitHub.nosync/monorepo/node_modules/.pnpm/node_modules"
else
  export NODE_PATH="/Users/jannesblobel/Documents/GitHub.nosync/monorepo/inlang/source-code/paraglide/paraglide-js/bin/node_modules:/Users/jannesblobel/Documents/GitHub.nosync/monorepo/inlang/source-code/paraglide/paraglide-js/node_modules:/Users/jannesblobel/Documents/GitHub.nosync/monorepo/inlang/source-code/paraglide/node_modules:/Users/jannesblobel/Documents/GitHub.nosync/monorepo/inlang/source-code/node_modules:/Users/jannesblobel/Documents/GitHub.nosync/monorepo/inlang/node_modules:/Users/jannesblobel/Documents/GitHub.nosync/monorepo/node_modules:/Users/jannesblobel/Documents/GitHub.nosync/node_modules:/Users/jannesblobel/Documents/node_modules:/Users/jannesblobel/node_modules:/Users/node_modules:/node_modules:/Users/jannesblobel/Documents/GitHub.nosync/monorepo/node_modules/.pnpm/node_modules:$NODE_PATH"
fi
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../@inlang/paraglide-js/bin/run.js" "$@"
else
  exec node  "$basedir/../@inlang/paraglide-js/bin/run.js" "$@"
fi
